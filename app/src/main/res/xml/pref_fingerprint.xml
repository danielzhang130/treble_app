<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <Preference
        android:key="key_test_fp"
        android:title="Find valid fingerprints" />
    <PreferenceCategory android:title="Advanced settings">
        <EditTextPreference
            android:key="key_fingerprint"
            android:title="Fingerprint"
            tools:summary="fingerprint/"/>
        <EditTextPreference
            android:key="key_product"
            android:title="Product"
            tools:summary="phone"/>
        <EditTextPreference
            android:key="key_device"
            android:title="Device"
            tools:summary="device"/>
        <EditTextPreference
            android:key="key_manufacturer"
            android:title="Manufacturer"
            tools:summary="manufacturer"/>
        <EditTextPreference
            android:key="key_brand"
            android:title="Brand"
            tools:summary="brand"/>
        <EditTextPreference
            android:key="key_model"
            android:title="Model"
            tools:summary="model"/>
        <Preference
            android:key="key_restart_gms"
            android:title="Restart GMS" />
        <Preference
            android:key="key_reset"
            android:title="Reset test record" />
    </PreferenceCategory>
</PreferenceScreen>
